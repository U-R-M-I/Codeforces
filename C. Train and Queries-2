#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);
     int t;
    cin>>t;
    while(t--)
    {
        int n,k;
        cin>>n>>k;
        vector<int> v(n);
        map<int,vector<int>> mp;
 
        for(int i=0; i<n; i++)
        {
            cin>>v[i];
            mp[v[i]].push_back(i);
        }
        for(auto[x,y]:mp)
        {
            sort(mp[x].begin(),mp[x].end());
        }
 
        while(k--)
        {
            int a,b;
            cin>>a>>b;
            if(mp[a].size()==0 || mp[b].size()==0)
            {
                cout<<"NO"<<endl;
                continue;
            }
 
            if(*mp[a].begin()<*mp[b].rbegin())
            {
                cout<<"YES"<<endl;
            }
            else
            {
                cout<<"NO"<<endl;
            }
        }
    }
    return 0;
}
