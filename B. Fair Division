#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        int cnt1 = 0, cnt2 = 0, x;
        for (int i = 0; i < n; i++) {
            cin >> x;
            if (x == 1) cnt1++;
            else cnt2++;
        }
        int sum = cnt1 + 2 * cnt2;
        if (sum % 2 != 0) {
            cout << "NO\n";
        } else {
            int half = sum / 2;
            if (half % 2 == 0 || cnt1 > 0)
                cout << "YES\n";
            else
                cout << "NO\n";
        }
    }
    return 0;
}
