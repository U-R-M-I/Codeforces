#include <bits/stdc++.h>
using namespace std;
 
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
 
    int t;
    cin >> t;
 
    while (t--) {
        int score = 0;
        for (int i = 0; i < 10; i++) {
            string row;
            cin >> row;
            for (int j = 0; j < 10; j++) {
                if (row[j] == 'X') {
                    int dist = min({i, j, 9 - i, 9 - j});
                    score += dist + 1;  // ring value
                }
            }
        }
        cout << score << "\n";
    }
 
    return 0;
}
