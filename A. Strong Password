#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);
    int n;
    cin>>n;
    map<char,bool>mp;
     string s;
     char x;
    for(int i=0;i<n;i++)
    {
        cin>>s;
         int a = -1;
 
        for(int i = 0; i < s.size() - 1; i++)
        {
             if(s[i] == s[i+1])
                a = i;
        }
          
        if(s[max(a, 0)] == 'z')
        {
              s.insert(a+1, 1, s[max(0, a)]-1);
        }
           
        else
        {
              s.insert(a+1, 1, s[max(0, a)]+1);
        }
          
 
        cout << s << endl;
 
      }
     
 }
