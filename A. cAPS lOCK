#include <bits/stdc++.h>
using namespace std;
int main()
{
     ios::sync_with_stdio(false);
     cin.tie(nullptr);
     string s;
     cin >> s;
     s[0] = toupper(s[0]);
     for (int i = 1; i < s.length(); i++)
     {

          s[i] = tolower(s[i]);
     }
     cout << s << '\n';
     return 0;
}
