#include <bits/stdc++.h>
using namespace std;
int main()
{
     int n;
     cin >> n;
    long long int f, factorial = 1;
     while (n--)
     {
          cin >> f;
          for (int i = 1; i <= f; i++)
          {
               factorial = factorial * i;
                    }
          cout << factorial << endl;
          factorial = 1;
     }
 
     return 0;
}
