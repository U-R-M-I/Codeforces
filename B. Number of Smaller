#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n, m;
    cin >> n >> m;

    vector<long long> A(n), B(m);
    for (int i = 0; i < n; i++) cin >> A[i];
    for (int i = 0; i < m; i++) cin >> B[i];

    vector<int> result(m);
    int j = 0; 
    for (int i = 0; i < m; i++) {
        while (j < n && A[j] < B[i]) {
            j++;
        }
        result[i] = j;
    }

    for (int i = 0; i < m; i++) {
        cout << result[i] << " ";
    }
    cout << "\n";

    return 0;
}
