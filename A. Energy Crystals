#include <bits/stdc++.h>
using namespace std;
using ll = long long;
 
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
 
    int t;
    cin >> t;
    while (t--)
    {
        ll x;
        cin >> x;
        ll yo = (ll)log2(x);
        ll ans = (2ll * (yo + 1)) + 1;
        cout << ans << "\n";
    }
 
    return 0;
}
