#include <bits/stdc++.h>
using namespace std;
int main()
{
     char c;
     cin >> c;
     if (c >= 'a' && c <= 'z')
     {
          char s = toupper(c);
          cout << s << endl;
     }
     else if (c >= 'A' && c <= 'Z')
     {
          char p = tolower(c);
          cout << p << endl;
     }
     return 0;
}
