#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define ll long long int
#define ull unsigned long long int
using namespace std;
void solve()
{
    int h,m;
    cin >> h >> m;
    cout << ((23-h)*60)+(60-m) << '\n';
}
int main()
{
    fast;
    int t;
    cin >> t;
    while(t--)
        solve();
    return 0;
}
