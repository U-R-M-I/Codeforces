#include <bits/stdc++.h>
using namespace std;
int main()
{
     int pas = 1999;
     int n;
     while (cin >> n)
     {
          if (n == pas)
          {
               cout << "Correct" << endl;
               break;
          }
          else
          {
               cout << "Wrong" << endl;
          }
     }
     return 0;
}
