#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        vector<string> grid(8);
        for (int i = 0; i < 8; i++) cin >> grid[i];
        
        string word = "";
        for (int col = 0; col < 8; col++) {
            // Check if this column contains a letter
            for (int row = 0; row < 8; row++) {
                if (grid[row][col] != '.') {
                    // Found the word column â€” collect all letters
                    for (int k = row; k < 8; k++) {
                        if (grid[k][col] == '.') break;
                        word += grid[k][col];
                    }
                    break;
                }
            }
            if (!word.empty()) break; // Word found, no need to check other columns
        }
        cout << word << "\n";
    }
    return 0;
}
