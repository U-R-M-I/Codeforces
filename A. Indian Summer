#include <bits/stdc++.h>
using namespace std;
int main()
{
     ios::sync_with_stdio(false);
     cin.tie(nullptr);
     int n;
     cin >> n;
     map<pair<string, string>, bool> s;
     for (int i = 0; i < n; i++)
     {
          string tree, leaf;
          cin >> tree >> leaf;
          s.insert({{leaf, tree}, true});
     }
     int ans = s.size();
     cout << ans << endl;
 
     return 0;
}
