#include <bits/stdc++.h>
using namespace std;
int main()
{
   string s;
   cin>>s;
    for (char& c : s) {
        if (isupper(c))
            c = tolower(c);
        else if (islower(c))
            c = toupper(c);
        else if(c==',')
        {
            c=' ';
        }
    }
    cout<<s<<endl;
     return 0;
}
