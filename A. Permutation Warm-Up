#include <bits/stdc++.h>
using namespace std;
 
void solve() {
    long long n, sum=0;
    cin >> n;
    for (int i=1; i<=n; i++) {
        sum+=abs(i-(n+1-i));
    }
 
    cout << sum/2+1 << "\n";
}
int main() {
	ios_base::sync_with_stdio(false);
    int w;
    cin >> w;
    while (w--) {
        solve();
    }
}
