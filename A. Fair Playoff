#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        vector<int> s(4);
        for (int i = 0; i < 4; i++) cin >> s[i];

        int w1 = max(s[0], s[1]);
        int w2 = max(s[2], s[3]);

        vector<int> sorted = s;
        sort(sorted.begin(), sorted.end());

        int top1 = sorted[3];
        int top2 = sorted[2];

        if ((w1 == top1 && w2 == top2) || (w1 == top2 && w2 == top1))
            cout << "YES\n";
        else
            cout << "NO\n";
    }
    return 0;
}
