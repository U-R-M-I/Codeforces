#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int a;
    cin >> a;
 
    while (a--) {
        int d;
        cin >> d;
 
        cout << 1 << ' ';
 
        int h = 3;
        while (h <= d) {
            cout << h << ' ';
            h++;
        }
 
        cout << 2 << endl;
    }
}
