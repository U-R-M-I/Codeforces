#include <bits/stdc++.h>
using namespace std;
 
int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    string s;
    while (cin >> s) {
        bool A = 0, B = 0, C = 0;
 
        for (auto ch : s) {
            if (ch == 'A') A = 1;
            else if (ch == 'B') B = 1;
            else if (ch == 'C') C = 1;
        }
 
        if (A && B && C) {
        }
        else {
            if (A && C && !B) cout << 'B' << endl;
            if (A && B && !C) cout << 'C' << endl;
            if (B && C && !A) cout << 'A' << endl;
        }
    }
 
    return 0;
}
