#include<bits/stdc++.h>
using namespace std;
 
#define ll long long
#define nl '\n'
 
 
int main () {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;   cin >> t;
    while (t--) {
        string s;   cin >> s;
        string e;
        for (int i = 0; i < s.size(); i++) {
            if (s[i] == 'T')e.push_back(s[i]);
        }
        for (auto it: e) cout << it;
        for (int i = 0; i < s.size(); i++) {
            if (s[i] != 'T')cout << s[i];
        }
        cout << nl;
    }    
}
