#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int t;
    cin >> t;
    while (t--) {
        string s;
        cin >> s;
        
        int A = 0, B = 0, C = 0;
        for (char c : s) {
            if (c == 'A') A++;
            else if (c == 'B') B++;
            else if (c == 'C') C++;
        }
        
        if (B == (A + C)) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }
}
