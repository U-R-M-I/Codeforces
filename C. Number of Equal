#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n, m;
    cin >> n >> m;
    vector<long long> a(n), b(m);
    for (int i = 0; i < n; i++) cin >> a[i];
    for (int j = 0; j < m; j++) cin >> b[j];

    long long ans = 0;
    int i = 0, j = 0;

    while (i < n && j < m) {
        if (a[i] < b[j]) {
            i++;
        } else if (a[i] > b[j]) {
            j++;
        } else {
            long long x = a[i];
            long long cntA = 0, cntB = 0;

            while (i < n && a[i] == x) {
                cntA++;
                i++;
            }
            while (j < m && b[j] == x) {
                cntB++;
                j++;
            }

            ans += cntA * cntB;
        }
    }

    cout << ans << "\n";
    return 0;
}
