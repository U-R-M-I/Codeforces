#include <bits/stdc++.h>
using namespace std;
int main()
{
     int a, b, c;
     cin >> a >> b >> c;
     int p = a + b * c;
     int d = a * b + c;
     int l = a + b + c;
     int r = a * b * c;
     int q = a * (b + c);
     int k = a + (b * c);
     int s = (a + b) * c;
     int j = (a * b) + c;
     int max_val = max({p, d, l, k, j, q, r, s});

     cout << max_val << endl;
     return 0;
}
