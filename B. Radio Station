#include<bits/stdc++.h>
using namespace std;
 
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
 
    int n , m ;
    cin >> n >> m;
    map<string,string>my_map ;
    for(int i = 0 ; i<n ; i++){
        string name , ip ;
        cin >> name >> ip ;
        ip+=';' ;
        my_map.insert({ip,name}) ;
    }
    for(int i = 0 ; i<m ; i++){
        string name , ip ;
        cin >> name >> ip ;
        cout << name <<" "<<ip<<" "<<'#'<<my_map[ip]<<endl ;
 
    }
 
    return 0;
}
